<%= form_with model: category do |form| %>
  <div>
    <%= form.label :category %><br>
    <%= form.text_field :category %>
    <% @category.errors.full_messages_for(:category).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>